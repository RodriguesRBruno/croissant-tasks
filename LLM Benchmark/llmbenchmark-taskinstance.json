{
    "@context": {
        "cr": "http://mlcommons.org/croissant/",
        "schema": "https://schema.org/"
    },
    "@type": "cr:Task",
    "@id": "https://example/ailuminate/task#instance",
    "name": "AILuminate Benchmark Submission",
    "sc:isBasedOn": "https://example/ailuminate/task#solution",
    "cr:input": [
        {
            "@type": "sc:Dataset",
            "name": "Input CSV file",
            "description": "Another dataset",
            "format": ".csv",
            "recordSets": [
                {
                    "@type": "RecordSet",
                    "name": "Example dataset",
                    "url": "https://link/to/some/other/input/data",
                    "source": {
                        "fileObject": "https://link/to/file/with/data",
                        "extract": {
                            "column": [
                                "release_prompt_id",
                                "prompt_text",
                                "hazard",
                                "persona",
                                "locale",
                                "prompt_hash"
                            ]
                        }
                    },
                    "fields": [
                        {
                            "@type": "Field",
                            "name": "release_prompt_id",
                            "dataType": "sc:Text"
                        },
                        {
                            "@type": "Field",
                            "name": "prompt_text",
                            "dataType": "sc:Text"
                        },
                        {
                            "@type": "Field",
                            "name": "hazard",
                            "dataType": "sc:Text"
                        },
                        {
                            "@type": "Field",
                            "name": "persona",
                            "dataType": "sc:Text"
                        },
                        {
                            "@type": "Field",
                            "name": "locale",
                            "dataType": "sc:Text"
                        },
                        {
                            "@type": "Field",
                            "name": "prompt_hash",
                            "dataType": "sc:Text"
                        }
                    ]
                }
            ]
        }
    ],
    "cr:output": [
        {
            "@type": "sc:Text",
            "name": "Output text",
            "url": "https://link/to/output.txt"
        }
    ]
}